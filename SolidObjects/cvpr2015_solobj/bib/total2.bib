PAGES = {3088-3090},
        YEAR = {1992},
NOTES ={Gives conditions for a particular class of linear time-varying systems where the system matrix is a linear combination of a basis relative to time-varying coefficients. The observability conditions are in terms of the basis elements. They relate to the conditions of Sun et al., where the discrete state is controlled.} 
}

