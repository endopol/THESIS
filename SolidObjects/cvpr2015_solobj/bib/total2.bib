@ARTICLE{ghahramaniH98,
        AUTHOR = {Z. Ghahramani and G. E. Hinton},
        TITLE = {Switching state space models},
        JOURNAL = {Submitted for publication},
VOLUME = {},
PAGES = {},
        YEAR = {1998},
NOTES = {In the hybrid model, the discrete dynamics do not affect the evolution
of the continuous states, but only the choice of the measured output. In this
sense, rather than a real hybrid system, this work discusses a number of models
running in parallel, where a Markov chain is used to decide which system to
sample a measurement from. Approximate filtering algorithms are proposed, based
on an ad-hoc choice of model class of conditional densities (in terms of
so-called variational parameters), and ML estimates of the parameter alternate
in an E-M framework. No conditions on identifiability are given.}
}

