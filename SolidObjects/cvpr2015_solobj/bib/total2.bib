@ARTICLE{sunGL02,
        AUTHOR = {A. Sun and S. S. Ge and T. H. Lee},
        TITLE = {Controllability and reachability criteria for switched linear
systems},
        JOURNAL = {Automatica},
VOLUME = {38},
PAGES = {775-786},
        YEAR = {2002},
NOTES = {Gives an unusual and somewhat limited conditions for observability in
terms of existence of a discrete state trajectory. In particular, instead of
observability being characterized as the existece of a time such that both the
continuous and the discrete state can be determined from the output,
observability is characterized as the existence of a time and a switching path
such that the continuous state alone can be observed. Therefore, these
conditions pertain to systems where the discrete state is controlled, rather
than evolve out of its own dynamics.}
}

@INPROCEEDINGS{szigeti92,
        AUTHOR = {F. Szigeti},
        TITLE = {A differential algebraic condition for controllability and
observability of time varying linear systems},
        BOOKTITLE = {Proc. of the Conf. on Decision and Control},
PAGES = {3088-3090},
        YEAR = {1992},
NOTES ={Gives conditions for a particular class of linear time-varying systems
where the system matrix is a linear combination of a basis relative to
time-varying coefficients. The observability conditions are in terms of the
basis elements. They relate to the conditions of Sun et al., where the discrete
state is controlled.}
}

