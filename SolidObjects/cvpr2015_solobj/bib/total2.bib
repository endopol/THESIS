@INPROCEEDINGS{hoYLLK03,
        AUTHOR = {J. Ho and M.-H. Yang and J. Lim and K.-C. Lee and D. Kriegman},
        TITLE = {Clustering appearances of objects under varying illumination conditions},
        BOOKTITLE = cvpr,
	PAGES = {},
        YEAR = {2003},
NOTES = {2 algotithms to cluster images of Lambertian scenes based on their identity, invariant with respect to illumination. Use the fact that images of Lambertian objects under varying illumination form linear subspaces -> Idea: find K polyhedial cones (or linear subspaces) that fit the data. "Illumination cones" is a bundle-like representation of objects under varying illumination. "Conic affinity" = distance in the equivalence class; the entire database is necessary in order to compute affinity between 2 images. Second method based on processing gradients; aims at small sensitivity (as opposed to invariance) to illumination. Pose is fixed. Exploits the fact that faces have very similar geometry; dismisses using this to estimate direction of incoming light (what if there are more light sources?).. Clustering is "unsupervised" (images are unlabeled). In the process they explain a positivity-constrained least-squares fitting, and a K-subspace clustering algorithm.}
}

