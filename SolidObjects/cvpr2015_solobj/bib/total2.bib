@InProceedings{yezziTW99,
  author =	 {A.~J.~Yezzi and A.~Tsai and A.~Willsky},
  title =	 {A statistical approach to snakes for bimodal and
                  trimodal imagery},
  booktitle =	 iccv,
  pages =	 {898--903},
  year =	 1999,
  volume =	 2,
  abstract =	 {We describe a new region based approach to active
                  contour for segmenting images composed of two or three
                  types of regions characterizable by a given
                  statistic. The essential idea is to derive curve
                  evolutions which seperate two or more valves of a
                  pre-determined set of statistics computed over
                  geometrically determined subsets of the image. Both
                  global and local image information is used to evolve
                  the active contour. Image derivatives, however, are
                  avoided, thereby giving rise to a further degree of
                  noise robustness compared to most edge based snake
                  aglorithms.}
}

