@ARTICLE{logothetisK99,
        AUTHOR = {A. Logothetis and V. Krishnamurthy},
        TITLE = {Expectation maximization algorithms for MAP estimation of jump
Markov linear systems},
        JOURNAL = {IEEE Trans. on Signal Processing},
VOLUME = {47(8)},
PAGES = {2139--2156},
        YEAR = {1999},
NOTES = {Proposes E-M algorithms for estimating continuous and/or discrete
states that are optimal in the MAP sense. The algorithms consist in various
combination of a Kalman smoother and a Viterbi algorithm. The particular choice
of inference criterion (MAP) makes it so that the conditinal densities estimated
are Gaussian, and therefore they can be estimated exactly (?). The same authors
have later also considered alternating minimization schemes to also estimate
model parameters as well as MCMC schemes to guarantee global convergence. There
is, however, no analysis of the conditions under which an estimate actually
exists or is unique.}
}
